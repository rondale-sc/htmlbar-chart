<div class="inline">
  <p>Template</p>
  {{textarea value=daTemplate classNameBindings='templateParseError' }}
</div>
<div class="inline">
  <p>Context</p>
  {{textarea value=daContext classNameBindings='jsonParseError' }}
</div>
<hr/>

<div class="inline">
  <h2>Handlebars</h2>
  <p>Render Time: {{handlebarsRenderTime}} ms</p>
  <div class="precompile">
    <code class="hljs javascript">
      {{{highlightedHandlebars}}}
    </code>
  </div>
  {{ view 
    class="rendered"
    instrumentName="render-time.handlebars"
    template=handlebarsTemplate
    context=jsonContext }}
</div>
<div class="inline">
  <h2>HTMLbars</h2>
  <p>Render Time: {{htmlbarsRenderTime}} ms</p>
  <div class="precompile">
    <code class="hljs javascript">
      {{{highlightedHTMLBars}}}
    </code>
  </div>
  {{ view 
    class="rendered"
    instrumentName="render-time.htmlbars"
    template=htmlbarsTemplate
    context=jsonContext }}
</div>
